        <messages/>
        <name>Summary</name>
        <order/>
        <published_ref/>
        <script><![CDATA[function onSubmit() {
  // Get MRVS value (JSON string)
  var mrvs = g_form.getValue('your_order_items'); // internal name of MRVS

  if (mrvs) {
    var rows = JSON.parse(mrvs);
    var summary = '';

    rows.forEach(function(row, index) {
      summary += "Item " + (index + 1) + ": " + row.menu_item +
                 " x " + row.quantity +
                 " (₹" + row.price + ", Total: ₹" + row.total + ")";
      if (row.special_instructions) {
        summary += " | Notes: " + row.special_instructions;
      }
      summary += "\n";
    });

    // Set hidden summary variable
    g_form.setValue('order_summary', summary);
  }
  return true; // allow submission
}
]]></script>
