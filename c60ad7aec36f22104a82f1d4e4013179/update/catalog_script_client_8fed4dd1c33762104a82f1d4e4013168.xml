        <name>Update Total on Quantity Change</name>
        <order/>
        <published_ref/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading) {
        return;
    }

    var price = parseFloat(g_form.getValue('price')) || 0;
    var quantity = parseInt(newValue, 10) || 0;

    var total = price * quantity;
    g_form.setValue('total', total.toFixed(2));
	g_form.setValue('final_order_total', total.toFixed(2));
}]]></script>
